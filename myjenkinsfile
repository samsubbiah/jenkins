pipeline{


agent  any

tools{

maven 'M3'

} 

stages{
	stage('checkout'){
	
	steps{
		sh 'git clone https://github.com/samsubbiah/jenkins.git'
	}

	}
	stage('compile'){

	steps{

	sh "mvn clean compile"
}
}

stage('test'){
steps{
sh 'mvn test'

}

}
	
stage('package'){
steps{
sh 'mvn package'

}


}



}




}
